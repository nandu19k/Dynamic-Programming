      "source": [
        "def climbingStairs(n):\n",
        "  nums = [0]*n\n",
        "  if n==1 or n==0:\n",
        "    return n\n",
        "  nums[0] = 1\n",
        "  nums[1] = 2\n",
        "\n",
        "  for i in range(2 , n):\n",
        "    nums[i] = nums[i-1] + nums[i-2]\n",
        "  return nums[-1]"
      ],
      "execution_count": 1,
      "outputs": []
    },
